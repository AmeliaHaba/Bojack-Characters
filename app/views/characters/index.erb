<h2>Bojack Horseman Characters</h2>
<% @characters.each do |character| %>
  <h4>Name: <a href="/characters/<%= character.id %>"><%= character.name %></a></h4>
  <h4>Gender: <%= character.gender %></h4>
  <h4>Animal: <%= character.animal? %></h4>
  <h4>Depressed: <%=character.depressed?%></h4>
<% end %>

<p><a href="/characters/new">create a new character</a></p>